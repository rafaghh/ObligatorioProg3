@model IEnumerable<ObligatorioProg3.Models.TipoSocio>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Beneficios)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TipoNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Beneficios)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTipoSocio">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTipoSocio">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTipoSocio">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
